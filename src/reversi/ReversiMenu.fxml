<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" spacing="10" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="reversi.MenuController">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Reversi" textAlignment="CENTER">
            <VBox.margin>
                <Insets left="190.0" top="20.0"/>
            </VBox.margin>
            <font>
                <Font name="Abyssinica SIL" size="67.0"/>
            </font>
        </Text>
        <Button id="start" fx:id="start" mnemonicParsing="false" onAction="#gameStart" prefHeight="40.0"
                prefWidth="105.0" text="Start">
            <VBox.margin>
                <Insets left="240.0" top="30.0"/>
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Button>
        <Button id="settings" fx:id="settings" mnemonicParsing="false" onAction="#gameSettings" prefHeight="40.0"
                prefWidth="105.0" text="Settings">
            <VBox.margin>
                <Insets left="240.0" top="20.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="exit" mnemonicParsing="false" onAction="#gameExit" prefHeight="40.0" prefWidth="105.0"
                text="Exit">
            <VBox.margin>
                <Insets left="240.0" top="20.0"/>
            </VBox.margin>
        </Button>
    </children>
</VBox>
